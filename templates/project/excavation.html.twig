{% extends "/dashboard-layout.html.twig" %}

	{% block head %}

	<!-- Specific Page Vendor CSS -->
	<link rel="stylesheet" href="assets/vendor/pnotify/pnotify.custom.css" />

	{% endblock %}

				{% block title %}
				<h2> L'excavation </h2>
				{% endblock %}

				{% block pageactuelle %} Excavation {% endblock %}

    {% block body %}

	<div class="row">
							<div class="col-xs-12">
									<header class="panel-heading">
									<h2 class="panel-title">L'excavation</h2>
									</header>
													<div class="form-group">
													{{ form_start(form) }}
													<br><br>
															{{ form_label(form.typeTerrassement, "Quel est le type de terrassement de votre terrain ? ", {'label_attr': {'class': 'col-sm-3 control-label'}}) }}													
														<div class="col-sm-9">
															{{ form_row(form.typeTerrassement, {'attr': {'class': 'form-control input-lg mb-md'}} ) }}           
														
														</div>
														
														{{ form_label(form.mursPeripherique, "Quel est la longueur des murs phériphériques ? ", {'label_attr': {'class': 'col-sm-3 control-label'}}) }}													
														<div class="col-sm-9">
															{{ form_row(form.mursPeripherique, {'attr': {'class': 'form-control input-lg mb-md'}} ) }}           
														
														</div>
														
														{{ form_label(form.mursRefont, "Quel est la longueur des murs de refont ? ", {'label_attr': {'class': 'col-sm-3 control-label'}}) }}													
														<div class="col-sm-9">
															{{ form_row(form.mursRefont, {'attr': {'class': 'form-control input-lg mb-md'}} ) }}           
														
														</div>
														
														{{ form_label(form.largeurFouille, "Quel est la largeur des fouilles ? ", {'label_attr': {'class': 'col-sm-3 control-label'}}) }}													
														<div class="col-sm-9">
															{{ form_row(form.largeurFouille, {'attr': {'class': 'form-control input-lg mb-md'}} ) }}           
														
														</div>
														
														{{ form_label(form.profondeurFouille, "Quel est la profondeur des fouilles ? ", {'label_attr': {'class': 'col-sm-3 control-label'}}) }}													
														<div class="col-sm-9">
															{{ form_row(form.profondeurFouille, {'attr': {'class': 'form-control input-lg mb-md'}} ) }}           
														
														</div>
														


														
														<!--
														{{ form_label(form.typeTerrassement, "Quel est le type de terrassement de votre terrain ? ", {'label_attr': {'class': 'col-sm-3 control-label'}}) }}													
														<div class="col-sm-9">
															{{ form_row(form.typeTerrassement, {'attr': {'class': 'form-control input-lg mb-md'}} ) }}           
														
														</div>
														-->

														<div id="nano"> 

														</div>

														</div>
														  <button class="mb-xs mt-xs mr-xs btn btn-lg btn-default" type="submit">Valider</button>
														   {{ form_end(form) }}
													</div>
													<br>
									
									
									</div>
									<div class="panel-footer">
										<ul class="pager">

										</ul>
									</div>
								</section>
							</div>
						</div>

    {% endblock %}

    {% block javascript %}
	<!-- Specific Page Vendor -->
		<script src="assets/vendor/jquery-ui/js/jquery-ui-1.10.4.custom.js"></script>
		<script src="assets/vendor/jquery-ui-touch-punch/jquery.ui.touch-punch.js"></script>
		<script src="assets/vendor/store-js/store.js"></script>
		<!--
		<script src="assets/public/js/app.js"></script>
		<script src="assets/public/js/runtime.js"></script>
		-->

    {% endblock %}